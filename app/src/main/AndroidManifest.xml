<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.group5_to_day">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Group5today">
        <activity
            android:name=".CreateLabelActivity"
            android:label="@string/create_label_label"
            android:parentActivityName=".ViewLabelsActivity" />
        <activity
            android:name=".ViewLabelsActivity"
            android:label="@string/view_labels_label"
            android:parentActivityName=".ViewTasksActivity"/>
        <activity
            android:name=".ViewTasksActivity"
            android:label="@string/view_tasks_activity_label" />
        <activity
            android:name=".ViewTaskDetails"
            android:label="@string/task_detail_title"
            android:parentActivityName=".ViewTasksActivity" />
        <activity
            android:name=".ViewTasksToEditActivity"
            android:label="@string/task_edit_label"
            android:parentActivityName=".ViewTasksActivity" />
        <activity
            android:name=".ViewCheckedTasksActivity"
            android:label="@string/view_checked_tasks_label"
            android:parentActivityName=".ViewTasksActivity" />
        <activity
            android:name=".EditTaskActivity"
            android:label="@string/task_edit_title"
            android:parentActivityName=".ViewTasksActivity" />
        <activity
            android:name=".RegisterUserActivity"
            android:label="@string/register_user_label"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CreateTaskActivity"
            android:label="@string/create_task_label"
            android:parentActivityName=".ViewTasksActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>